<% @dns_servers.each do |dns| %>dns <%= dns %><% end %>
<% @maps.each do |map| %>map_load <%= map %><% end %>
<% @maps.each do |map| %>sv_mapcycle_add <%= map %> "<%= @game_type %>" <% end %>
<% if @ping_kick %>ping_kick <%= @ping_kick %><% end %>
<% if @afk_kick %>afk_kick <%= @afk_kick %><% end %>
<% if @collect_aliases %>collect_aliases <%= @collect_aliases %><% end %>
<% if @spawn_protection %>spawn_protection <%= @spawn_protection %><% end %>
<% if @antihalofp %>antihalofp <%= @antihalofp %><% end %>
<% if @anti_warp %>antiwarp <%= @anti_warp %><% end %>
<% if @lua %>lua <%= @lua %><% end %>
<% if @anti_caps %>anticaps <%= @anti_caps %><% end %>
<% if @anti_spam %>antispam <%= @anti_spam %><% end %>
<% if @no_lead %>no_lead <%= @no_lead %><% end %>
<% if @save_scores %>save_scores <%= @save_scores %><% end %>
log 1
log_name sapp
packet_limit 0